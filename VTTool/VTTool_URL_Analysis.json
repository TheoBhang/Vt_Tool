{
    "name": "VT_Tool_URL_Analysis",
    "version": "3.1",
    "author": "TBhang",
    "url": "https://github.com/TheHive-Project/Cortex-Analyzers",
    "license": "AGPL-V3",
    "description": "Get the latest VirusTotal reports for URLs in a file.",
    "dataTypeList": ["file"],
    "command": "VT_Tool/vt3_tools.py",
    "baseConfig": "VT_Tools",
    "config": {
      "service": "urls"
    },
    "configurationItems": [
      {
        "name": "key",
        "description": "API key for Virustotal",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "case_id",
        "description": "Case ID",
        "type": "number",
        "multi": false,
        "required": false,
        "defaultValue": 60
      },
      {
        "name": "threshold",
        "description": "Threshold for score to be really considered as malicious, suspicious or safe (0-100)\nPercentages of triggered AVs",
        "type": "number",
        "multi": false,
        "required": false,
        "defaultValue": 30
      }
    ],
    "service_logo": {
      "path": "assets/logo.png",
      "caption": "logo"
    }
  }
  